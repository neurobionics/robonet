[connection]
id={AP_SSID}
type=wifi
interface-name=wlan0
autoconnect=true
autoconnect-priority={AP_PRIORITY}
autoconnect-retries=1

[wifi]
band=bg
channel=7
mode=ap
ssid={AP_SSID}

[wifi-security]
key-mgmt=wpa-psk
psk={AP_PSK}

[ipv4]
address1={AP_IP_ADDRESS}/24
method=shared

[ipv6]
addr-gen-mode=default
method=disabled

[proxy]